<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.pf.entity.Parkin" table="act_parkin">
    	<meta attribute="class-description">入孵申请单</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="projectName" type="java.lang.String">
			<column name="project_name">
				<comment>项目名称</comment>
			</column>
		</property>
		<property name="projectId" type="java.lang.Long">
			<column name="project_id">
				<comment>项目ID</comment>
			</column>
		</property>
		<property name="processInstanceId" type="java.lang.String">
			<column name="processInstanceId">
				<comment>流程实例ID</comment>
			</column>
		</property>
		
		<property name="startUserId" type="java.lang.String">
			<column name="start_user_id">
				<comment>发起人ID</comment>
			</column>
		</property>
		<property name="startUserName" type="java.lang.String">
			<column name="start_user_name">
				<comment>发起人姓名</comment>
			</column>
		</property>
		
		<property name="applyTime" type="java.util.Date">
			<column name="applyTime">
				<comment>申请时间</comment>
			</column>
		</property>
		
		<property name="ywyOpinion" type="java.lang.String">
			<column name="ywy_opinion" length="512">
				<comment>业务员意见</comment>
			</column>
		</property>
		
		
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
