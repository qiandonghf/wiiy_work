<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.park.entity.Park">
    	<meta attribute="class-description">园区</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.TreeEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String" length="255" unique-key="true">
			<column name="name">
				<comment>园区名称</comment>
			</column>
		</property>
		<property name="water" type="java.lang.Double">
			<column name="water">
				<comment>水费</comment>
			</column>
		</property>
		<property name="electricity" type="java.lang.Double">
			<column name="electricity">
				<comment>电费</comment>
			</column>
		</property>
		<property name="gas" type="java.lang.Double">
			<column name="gas">
				<comment>气费</comment>
			</column>
		</property>
		<property name="energyFee" type="java.lang.Double">
			<column name="energy_fee">
				<comment>能源费</comment>
			</column>
		</property>
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="level" type="java.lang.Integer">
			<column name="level">
				<comment>LEVEL</comment>
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
