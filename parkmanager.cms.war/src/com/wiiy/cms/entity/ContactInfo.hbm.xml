<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.cms.entity.ContactInfo">
    	<meta attribute="class-description">联系方式</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="paramId" type="java.lang.Long">
            <column name="param_id">
                <comment>网站外键Id</comment>
            </column>
        </property>
		<property name="name" type="java.lang.String">
			<column name="name" >
				<comment>公司名称</comment>
			</column>
		</property>
		<property name="contact" type="java.lang.String">
			<column name="contact">
				<comment>联系人</comment>
			</column>
		</property>
		<property name="phone" type="java.lang.String">
			<column name="phone">
				<comment>电话</comment>
			</column>
		</property>
		<property name="telephone" type="java.lang.String">
			<column name="telephone">
				<comment>手机</comment>
			</column>
		</property>
		<property name="fax" type="java.lang.String">
			<column name="fax" >
				<comment>传真</comment>
			</column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="email">
				<comment>E-mail</comment>
			</column>
		</property>
		<property name="postCode" type="java.lang.String">
			<column name="post_code">
				<comment>邮政编码</comment>
			</column>
		</property>
		<property name="address" type="java.lang.String">
			<column name="address" length="255">
				<comment>地址</comment>
			</column>
		</property>
		
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
