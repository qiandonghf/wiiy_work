<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wiiy.sale.entity.ContectInfo">
        <meta attribute="class-description">来电/来访</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
        <property name="customerId" type="java.lang.Long">
			<column name="sale_customer_id">
				<comment>客户外键</comment>
			</column>
		</property>
		 <many-to-one name="customer" class="com.wiiy.sale.entity.SaleCustomer" foreign-key="fk_contectinfo_sale_customer" insert="false" update="false" fetch="join">
            <column name="sale_customer_id" not-null="true">
                <comment>客户</comment>
            </column>
        </many-to-one>
         <property name="receiverId" type="java.lang.Long">
			<column name="receiver_id">
				<comment>接待人员外键</comment>
			</column>
		</property>
        <many-to-one name="receiver" class="com.wiiy.core.entity.User" foreign-key="fk_salecontectInfo_user_receiver" insert="false" update="false" fetch="join">
            <column name="receiver_id">
                <comment>接待人员</comment>
            </column>
        </many-to-one>
        <property name="returnVisit" type="java.util.Date">
            <column name="return_visit">
                <comment>回访提醒</comment>
            </column>
        </property>
        <property name="returnTime" type="java.util.Date">
            <column name="return_time">
                <comment>上次回访时间</comment>
            </column>
        </property>
        <property name="time" type="java.util.Date">
            <column name="time">
                <comment>时间</comment>
            </column>
        </property>
        <property name="infoStatus">
        	<column name="info_status">
				<comment>状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.sale.preferences.enums.InfoEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<!-- <property name="startTime" type="java.util.Date">
            <column name="start_time">
                <comment>交往开始时间</comment>
            </column>
        </property>
        <property name="endTime" type="java.util.Date">
            <column name="end_time">
                <comment>交往结束时间</comment>
            </column>
        </property> -->
		<many-to-one name="type" class="com.wiiy.core.entity.DataDict" foreign-key="fk_sale_contectinfo_datadict_type" insert="false" update="false" fetch="join">
			<column name="type_id">
				<comment>交往类型</comment>
			</column>
		</many-to-one>
		<property name="typeId" type="java.lang.String">
			<column name="type_id">
				<comment>交往类型外键</comment>
			</column>
		</property>
        <property name="content" type="java.lang.String">
            <column name="content" sql-type="text">
                <comment>情况</comment>
            </column>
        </property>
        <property name="memo" type="java.lang.String" length="512">
            <column name="memo" sql-type="text">
                <comment>备注</comment>
            </column>
        </property>
        
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
