<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wiiy.business.entity.InvestmentRepeatedViste">
        <meta attribute="class-description">招商回访信息</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="investmentContectInfoId" type="java.lang.Long" not-null="true">
			<column name="investmentContectInfo_id">
				<comment>线索外键</comment>
			</column>
		</property>
		<many-to-one name="investmentContectInfo" class="com.wiiy.business.entity.InvestmentContectInfo" foreign-key="fk_InvestmentLog_investmentContectInfo" insert="false" update="false" fetch="join">
			<column name="investmentContectInfo_id">
				<comment>线索实体</comment>
			</column>
		</many-to-one>
		<property name="name" type="java.lang.String">
        	<column name="name">
        		<comment>姓名</comment>
        	</column>
        </property>
        <property name="phone" type="java.lang.String" length="50">
            <column name="phone">
                <comment>联系电话</comment>
            </column>
        </property>
        <property name="product" type="java.lang.String" length="50">
            <column name="product">
                <comment>产业</comment>
            </column>
        </property>
        <property name="demand" type="java.lang.String" length="50">
            <column name="demand">
                <comment>需求</comment>
            </column>
        </property>
        <property name="receiverId" type="java.lang.Long">
			<column name="receiver_id">
				<comment>回访人员外键</comment>
			</column>
		</property>
        <many-to-one name="receiver" class="com.wiiy.core.entity.User" foreign-key="fk_RepeatedViste_user_receiver" insert="false" update="false" fetch="join">
            <column name="receiver_id">
                <comment>回访人员</comment>
            </column>
        </many-to-one>
		<property name="startTime" type="java.util.Date">
            <column name="start_time">
                <comment>回访开始时间</comment>
            </column>
        </property>
        <property name="endTime" type="java.util.Date">
            <column name="end_time">
                <comment>回访结束时间</comment>
            </column>
        </property>
        <property name="memo" type="java.lang.String" length="512">
            <column name="memo" sql-type="text">
                <comment>回访情况</comment>
            </column>
        </property>
        
        <property name="remindTime" type="java.util.Date">
            <column name="remind_time">
                <comment>回访提醒时间</comment>
            </column>
        </property>
        
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
