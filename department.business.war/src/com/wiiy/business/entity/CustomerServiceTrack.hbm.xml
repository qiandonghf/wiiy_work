<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wiiy.business.entity.CustomerServiceTrack">
		<meta attribute="class-description">客服服务轨迹</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="serviceId" type="java.lang.Long" not-null="true">
			<column name="service_id">
				<comment>客服联系单ID</comment>
			</column>
		</property>
		<many-to-one name="service" class="com.wiiy.business.entity.CustomerService" foreign-key="fk_customerservicetrack_customer" insert="false" update="false" fetch="join">
            <column name="service_id" not-null="true">
                <comment>客服联系单</comment>
            </column>
        </many-to-one>
		<property name="userId" type="java.lang.Long" not-null="true">
			<column name="user_id">
				<comment>处理人ID</comment>
			</column>
		</property>
		<many-to-one name="user" class="com.wiiy.core.entity.User" foreign-key="fk_customerservicetrack_user" insert="false" update="false" fetch="join">
            <column name="user_id" not-null="true">
                <comment>处理人</comment>
            </column>
        </many-to-one>
        <property name="handleTime" type="java.util.Date" not-null="true">
			<column name="handle_time">
				<comment>受理开始日期</comment>
			</column>
		</property>
		<property name="content" type="java.lang.String" not-null="true">
			<column name="content">
				<comment>内容</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
