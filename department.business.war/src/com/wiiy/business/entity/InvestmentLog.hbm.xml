<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.business.entity.InvestmentLog">
    	<meta attribute="class-description">招商轨迹</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="investmentId" type="java.lang.Long" not-null="true">
			<column name="investment_id">
				<comment>招商项目外键</comment>
			</column>
		</property>
		<many-to-one name="investment" class="com.wiiy.business.entity.Investment" foreign-key="fk_InvestmentLog_investment" insert="false" update="false" fetch="join">
			<column name="investment_id">
				<comment>招商项目实体</comment>
			</column>
		</many-to-one>
		<property name="userId" type="java.lang.Long">
			<column name="user_id">
				<comment>业务员外键</comment>
			</column>
		</property>
		<property name="userName" type="java.lang.String">
			<column name="user_name">
				<comment>业务员姓名</comment>
			</column>
		</property>
		<property name="linkMan" type="java.lang.String">
			<column name="link_man">
				<comment>联系人</comment>
			</column>
		</property>
		<property name="linkTime" type="java.util.Date">
			<column name="link_time">
				<comment>时间</comment>
			</column>
		</property>
		<property name="content" type="java.lang.String">
			<column name="content">
				<comment>内容</comment>
			</column>
		</property>
		
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
