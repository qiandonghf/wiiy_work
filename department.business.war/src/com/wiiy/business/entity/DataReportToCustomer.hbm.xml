<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wiiy.business.entity.DataReportToCustomer">
        <meta attribute="class-description">报表上报企业配置</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
        <property name="reportId" type="java.lang.Long"  not-null="true">
			<column name="report_id">
				<comment>报表外键</comment>
			</column>
		</property>
        <many-to-one name="report" class="com.wiiy.business.entity.DataReport" foreign-key="fk_datareporttocustomer_report" insert="false" update="false" fetch="join">
            <column name="report_id" not-null="true">
                <comment>报表</comment>
            </column>
        </many-to-one>
        <property name="customerId" type="java.lang.Long"  not-null="true">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <many-to-one name="customer" class="com.wiiy.business.entity.BusinessCustomer" foreign-key="fk_datareporttocustomer_businesscustomer" insert="false" update="false" fetch="join">
            <column name="customer_id" not-null="true">
                <comment>企业</comment>
            </column>
        </many-to-one>
        <property name="fillTime" type="java.util.Date">
			<column name="fill_time">
				<comment>填写时间</comment>
			</column>
		</property>
		<property name="finished">
			<column name="finished" sql-type="varchar(50)">
				<comment>是否填写</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.BooleanEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
		<property name="status">
        	<column name="status">
				<comment>上报状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.business.preferences.enums.CustomerReportStatusEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
