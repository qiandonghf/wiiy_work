<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.business.entity.InvestmentInvestor">
    	<meta attribute="class-description">投资人</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="investmentId" type="java.lang.Long" not-null="true">
			<column name="investment_id">
				<comment>招商项目外键</comment>
			</column>
		</property>
        <many-to-one name="investment" class="com.wiiy.business.entity.Investment" foreign-key="fk_investmentinvestor_investment" insert="false" update="false" fetch="join">
            <column name="investment_id" not-null="true">
                <comment>招商项目</comment>
            </column>
        </many-to-one>
        <property name="name" type="java.lang.String" not-null="true">
        	<column name="name">
        		<comment>出资方</comment>
        	</column>
        </property>
        <property name="rate" type="java.lang.Double">
        	<column name="rate">
        		<comment>出资比例</comment>
        	</column>
        </property>
        <property name="birthDay" type="java.util.Date">
        	<column name="birth_day">
        		<comment>出生日期</comment>
        	</column>
        </property>
        <many-to-one name="capital" class="com.wiiy.core.entity.DataDict" foreign-key="fk_investmentinvestor_datadict_capital" insert="false" update="false" fetch="join">
			<column name="capital_id">
				<comment>出资方式</comment>
			</column>
		</many-to-one>
		<property name="capitalId" type="java.lang.String">
			<column name="capital_id">
				<comment>出资方式外键</comment>
			</column>
		</property>
		<many-to-one name="degree" class="com.wiiy.core.entity.DataDict" foreign-key="fk_investmentinvestor_datadict_degree" insert="false" update="false" fetch="join">
			<column name="degree_id">
				<comment>学位</comment>
			</column>
		</many-to-one>
		<property name="degreeId" type="java.lang.String">
			<column name="degree_id">
				<comment>学位外键</comment>
			</column>
		</property>
		
       
        <property name="school" type="java.lang.String">
        	<column name="school">
        		<comment>毕业学校</comment>
        	</column>
        </property>
        <property name="graduateYear" type="java.lang.Integer">
        	<column name="graduate_year">
        		<comment>毕业年份</comment>
        	</column>
        </property>
         <property name="profession" type="java.lang.String">
        	<column name="profession">
        		<comment>职称</comment>
        	</column>
        </property>
        
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
