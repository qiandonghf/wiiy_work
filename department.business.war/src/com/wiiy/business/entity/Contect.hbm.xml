<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wiiy.business.entity.Contect">
        <meta attribute="class-description">联系人</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
        <property name="customerId" type="java.lang.Long" not-null="true">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <many-to-one name="customer" class="com.wiiy.business.entity.BusinessCustomer" foreign-key="fk_contect_customer_customer" insert="false" update="false" fetch="join">
            <column name="customer_id" not-null="true">
                <comment>企业</comment>
            </column>
        </many-to-one>
        <property name="name" type="java.lang.String" not-null="true">
            <column name="name" length="50">
                <comment>姓名</comment>
            </column>
        </property>
        <property name="gender">
        	<column name="gender">
				<comment>性别</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.GenderEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="mobile" type="java.lang.String">
            <column name="mobile">
                <comment>手机号码</comment>
            </column>
        </property>
        <property name="main">
			<column name="main" sql-type="varchar(50)">
				<comment>是否首要联系人</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.BooleanEnum</param>
				<param name="type">12</param>
			</type>	
		</property>
		<property name="position" type="java.lang.String" length="20">
            <column name="position">
                <comment>职位</comment>
            </column>
        </property>
		<property name="email" type="java.lang.String" length="255">
            <column name="email">
                <comment>Email</comment>
            </column>
        </property>
        <property name="phone" type="java.lang.String" length="50">
            <column name="phone">
                <comment>电话</comment>
            </column>
        </property>
        <property name="msn" type="java.lang.String" length="255">
            <column name="msn">
                <comment>MSN</comment>
            </column>
        </property>
        <property name="qq" type="java.lang.String" length="50">
            <column name="qq">
                <comment>QQ</comment>
            </column>
        </property>
        <property name="fax" type="java.lang.String" length="50">
            <column name="fax">
                <comment>传真</comment>
            </column>
        </property>
        <property name="zipcode" type="java.lang.String" length="50">
            <column name="zipcode">
                <comment>邮编</comment>
            </column>
        </property>
        <property name="homeAddr" type="java.lang.String" length="255">
            <column name="home_addr">
                <comment>家庭地址</comment>
            </column>
        </property>
        <property name="homePhone" type="java.lang.String" length="50">
            <column name="home_phone">
                <comment>家庭电话</comment>
            </column>
        </property>
        <property name="favorite" type="java.lang.String" length="50">
            <column name="favorite">
                <comment>爱好</comment>
            </column>
        </property>
        <property name="birthDay" type="java.util.Date">
            <column name="birth_day">
                <comment>生日</comment>
            </column>
        </property>
		
        <property name="memo" type="java.lang.String" length="512">
            <column name="memo" sql-type="text">
                <comment>备注</comment>
            </column>
        </property>
        
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
