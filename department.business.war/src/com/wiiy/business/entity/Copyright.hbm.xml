<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wiiy.business.entity.Copyright">
        <meta attribute="class-description">知识产权(著作权)</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
        <property name="customerId" type="java.lang.Long" not-null="true">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <many-to-one name="customer" class="com.wiiy.business.entity.BusinessCustomer" foreign-key="fk_copyright_customer_customer" insert="false" update="false" fetch="join">
            <column name="customer_id" not-null="true">
                <comment>企业</comment>
            </column>
        </many-to-one>
        <many-to-one name="type" class="com.wiiy.core.entity.DataDict" foreign-key="fk_copyright_datadict_type" insert="false" update="false" fetch="join">
			<column name="type_id" >
				<comment>著作权类型</comment>
			</column>
		</many-to-one>
		<property name="typeId" type="java.lang.String" not-null="true">
			<column name="type_id">
				<comment>著作权类型外键</comment>
			</column>
		</property>
        
        <property name="name" type="java.lang.String" not-null="true">
            <column name="name" length="50">
                <comment>著作权名称</comment>
            </column>
        </property>
        <property name="serialNo" type="java.lang.String">
            <column name="serial_no">
                <comment>著作权号</comment>
            </column>
        </property>
        <property name="appler" type="java.lang.String">
            <column name="appler">
                <comment>申请人</comment>
            </column>
        </property>
        <property name="applyTime" type="java.util.Date">
            <column name="apply_time">
                <comment>申请时间</comment>
            </column>
        </property>
        <property name="effectivetime" type="java.util.Date">
            <column name="effective_time">
                <comment>生效时间</comment>
            </column>
        </property>
        <property name="expireTime" type="java.util.Date">
            <column name="expire_time">
                <comment>失效时间</comment>
            </column>
        </property>
        <property name="summery" type="java.lang.String" length="512">
            <column name="summery" sql-type="text">
                <comment>摘要</comment>
            </column>
        </property>
        
        <property name="pub">
			<column name="pub" sql-type="varchar(50)">
				<comment>是否发布到网站</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.BooleanEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
        
        <property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
