<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wiiy.business.entity.CustomerService">
		<meta attribute="class-description">客服联系单</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="customerId" type="java.lang.Long">
			<column name="customer_id">
				<comment>企业ID</comment>
			</column>
		</property>
         <property name="customerName" type="java.lang.String">
			<column name="customer_name">
				<comment>企业名称</comment>
			</column>
		</property>
        <property name="contect" type="java.lang.String">
			<column name="contect">
				<comment>联系人</comment>
			</column>
		</property>
		<property name="serviceName" type="java.lang.String">
			<column name="serviceName">
				<comment>联系单名称</comment>
			</column>
		</property>
        <property name="orgId" type="java.lang.Long">
			<column name="org_id">
				<comment>受理部门ID</comment>
			</column>
		</property>
		<many-to-one name="org" class="com.wiiy.core.entity.Org" foreign-key="fk_customerservice_org" insert="false" update="false" fetch="join">
            <column name="org_id">
                <comment>受理部门</comment>
            </column>
        </many-to-one>
        <property name="userId" type="java.lang.Long">
			<column name="user_id">
				<comment>受理人ID</comment>
			</column>
		</property>
		<many-to-one name="user" class="com.wiiy.core.entity.User" foreign-key="fk_customerservice_user" insert="false" update="false" fetch="join">
            <column name="user_id">
                <comment>受理人</comment>
            </column>
        </many-to-one>
		<property name="typeId" type="java.lang.String" not-null="true">
			<column name="type_id">
				<comment>服务类型ID</comment>
			</column>
		</property>
		<many-to-one name="type" class="com.wiiy.core.entity.DataDict" foreign-key="fk_customerservice_datadict" insert="false" update="false" fetch="join">
            <column name="type_id" not-null="true">
                <comment>服务类型</comment>
            </column>
        </many-to-one>
        <property name="startDate" type="java.util.Date">
			<column name="startDate">
				<comment>受理开始日期</comment>
			</column>
		</property>
		<property name="endDate" type="java.util.Date">
			<column name="endDate">
				<comment>受理结束日期</comment>
			</column>
		</property>
		<property name="phone" type="java.lang.String" not-null="true">
			<column name="phone">
				<comment>联系电话</comment>
			</column>
		</property>
		<property name="status">
        	<column name="status">
				<comment>状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.business.preferences.enums.CustomerServiceStatusEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="priority">
			<column name="priority">
				<comment>重要性</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.business.preferences.enums.PriorityEnum</param>
				<param name="type">12</param>
			</type>
		</property>
        <property name="result">
        	<column name="result">
				<comment>服务结果</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.business.preferences.enums.CustomerServiceResultEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="description" type="java.lang.String">
			<column name="description">
				<comment>情况说明</comment>
			</column>
		</property>
		<property name="suggest" type="java.lang.String">
			<column name="suggest">
				<comment>客服意见及建议</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
