<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 
    <class name="com.wiiy.business.entity.Product">
        <meta attribute="class-description">企业产品</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="customerId" type="java.lang.Long"  not-null="true">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <many-to-one name="customer" class="com.wiiy.business.entity.BusinessCustomer" foreign-key="fk_product_customer_costomer" insert="false" update="false" fetch="join">
            <column name="customer_id" not-null="true">
                <comment>企业</comment>
            </column>
        </many-to-one>
        <many-to-one name="stage" class="com.wiiy.core.entity.DataDict" foreign-key="fk_product_datadict_stage" insert="false" update="false" fetch="join">
			<column name="stage_id" >
				<comment>产品阶段</comment>
			</column>
		</many-to-one>
		<property name="stageId" type="java.lang.String" not-null="true">
			<column name="stage_id">
				<comment>产品阶段外键</comment>
			</column>
		</property>
		<many-to-one name="technic" class="com.wiiy.core.entity.DataDict" foreign-key="fk_product_datadict_technic" insert="false" update="false" fetch="join">
			<column name="technic_id" >
				<comment>技术领域</comment>
			</column>
		</many-to-one>
		<property name="technicId" type="java.lang.String" not-null="true">
			<column name="technic_id">
				<comment>技术领域外键</comment>
			</column>
		</property>
       
       
        <property name="name" type="java.lang.String">
            <column name="name" not-null="true">
                <comment>产品名称</comment>
            </column>
        </property>
       
       
        <property name="introduction" type="java.lang.String">
            <column name="introduction" sql-type="text">
                <comment>产品简介</comment>
            </column>
        </property>
        <property name="pub">
			<column name="pub" sql-type="varchar(50)">
				<comment>是否发布到网站</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.BooleanEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
        <property name="photos" type="java.lang.String">
			<column name="photos" sql-type="text">
				<comment>产品照片</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
