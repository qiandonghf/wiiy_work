<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.activiti.entity.Leave" table="act_leave">
    	<meta attribute="class-description">请假单</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="processInstanceId" type="java.lang.String">
			<column name="processInstanceId">
				<comment>流程实例ID</comment>
			</column>
		</property>
		<property name="userId" type="java.lang.String">
			<column name="userId">
				<comment>用户ID</comment>
			</column>
		</property>
		<property name="startTime" type="java.util.Date">
			<column name="START_TIME">
				<comment>开始时间</comment>
			</column>
		</property>
		<property name="endTime" type="java.util.Date">
			<column name="END_TIME">
				<comment>结束时间</comment>
			</column>
		</property>
		<property name="applyTime" type="java.util.Date">
			<column name="applyTime">
				<comment>申请时间</comment>
			</column>
		</property>
		<property name="leaveType" type="java.lang.String">
			<column name="leaveType">
				<comment>请假类型</comment>
			</column>
		</property>
		<property name="reason" type="java.lang.String">
			<column name="reason" length="255">
				<comment>请假类型</comment>
			</column>
		</property>
		<property name="realityStartTime" type="java.util.Date">
			<column name="REALITY_START_TIME">
				<comment>实际开始时间</comment>
			</column>
		</property>
		<property name="realityEndTime" type="java.util.Date">
			<column name="REALITY_END_TIME">
				<comment>实际结束时间</comment>
			</column>
		</property>
		
		
		
		
		
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
