<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.estate.entity.SupplyPurchase">
    	<meta attribute="class-description">办公用品采购申请</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="supplyId" type="java.lang.Long">
			<column name="supply_id" not-null="true">
				<comment>办公用品外键</comment>
			</column>
		</property>
		<many-to-one name="supply" class="com.wiiy.estate.entity.Supply" foreign-key="fk_estate_supplyPurchase_supply" insert="false" update="false" fetch="join">
			<column name="supply_id">
				<comment>办公用品实体</comment>
			</column>
		</many-to-one>
		<property name="amount" type="java.lang.Double">
			<column name="amount" precision="2">
				<comment>数量</comment>
			</column>
		</property>
		<property name="price" type="java.lang.Double">
			<column name="price" precision="2">
				<comment>单价</comment>
			</column>
		</property>
		<property name="totalPrice" type="java.lang.Double">
			<column name="total_price" precision="2">
				<comment>总价</comment>
			</column>
		</property>
		<property name="applyTime" type="java.util.Date">
			<column name="apply_time">
				<comment>申请时间</comment>
			</column>
		</property>
		<property name="purchaser" type="java.lang.String">
			<column name="purchaser">
				<comment>购买人</comment>
			</column>
		</property>
		<property name="usages" type="java.lang.String">
			<column name="usages">
				<comment>物品用途</comment>
			</column>
		</property>
		<property name="memo" type="java.lang.String">
			<column name="memo">
				<comment>备注</comment>
			</column>
		</property>
		<property name="spec" type="java.lang.String">
			<column name="spec">
				<comment>型号</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
