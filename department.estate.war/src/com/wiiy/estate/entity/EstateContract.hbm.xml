<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 
    <class name="com.wiiy.estate.entity.EstateContract">
        <meta attribute="class-description">合同</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="customerId" type="java.lang.Long">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <many-to-one name="customer" class="com.wiiy.business.entity.BusinessCustomer" foreign-key="fk_Estate_contract_customer" insert="false" update="false" fetch="join">
            <column name="customer_id">
                <comment>企业</comment>
            </column>
        </many-to-one>
        <property name="contractPartyA" type="java.lang.String">
        	<column name="contractPartyA">
        		<comment>合同甲方</comment>
        	</column>
        </property>
        <property name="purpose" type="java.lang.String">
        	<column name="purpose">
        		<comment>租赁用途</comment>
        	</column>
        </property>
        <property name="policy" type="java.lang.String">
        	<column name="policy">
        		<comment>优惠政策</comment>
        	</column>
        </property>
        
        <property name="customerName" type="java.lang.String">
        	<column name="customer_name">
        		<comment>企业名称</comment>
        	</column>
        </property>
        <property name="managerId" type="java.lang.Long">
			<column name="manager_id">
				<comment>负责人外键</comment>
			</column>
		</property>
        <many-to-one name="manager" class="com.wiiy.core.entity.User" foreign-key="fk_Estate_contract_manager" insert="false" update="false" fetch="join">
            <column name="manager_id">
                <comment>负责人</comment>
            </column>
        </many-to-one>
        <property name="name" type="java.lang.String">
			<column name="name">
				<comment>合同名称</comment>
			</column>
		</property>
		<property name="code" type="java.lang.String">
			<column name="code" length="50">
				<comment>合同编号</comment>
			</column>
		</property>
		<property name="item">
			<column name="item" sql-type="varchar(50)">
				<comment>合同类别</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.estate.preferences.enums.ContractItemEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
		<property name="signDate" type="java.util.Date">
			<column name="sign_date" length="19">
				<comment>合同签订日期</comment>
			</column>
		</property>
		<property name="receiveDate" type="java.util.Date">
			<column name="receive_date" length="19">
				<comment>交付日期</comment>
			</column>
		</property>
		<property name="startDate" type="java.util.Date">
			<column name="start_date" length="19">
				<comment>合同有效期开始日期</comment>
			</column>
		</property>
		<property name="endDate" type="java.util.Date">
			<column name="end_date" length="19">
				<comment>合同有效期结束日期</comment>
			</column>
		</property>
		<property name="decorateStartDate" type="java.util.Date">
			<column name="decorate_start_date" length="19">
				<comment>装修开始日期</comment>
			</column>
		</property>
		<property name="decorateEndDate" type="java.util.Date">
			<column name="decorate_end_date" length="19">
				<comment>装修结束日期</comment>
			</column>
		</property>
        <property name="state">
			<column name="state" sql-type="varchar(50)">
				<comment>合同状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.estate.preferences.enums.ContractStatusEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
        <property name="rentState">
			<column name="rentState" sql-type="varchar(50)">
				<comment>租赁状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.estate.preferences.enums.ContractRentStatusEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
        <property name="payType">
			<column name="pay_type" sql-type="varchar(50)">
				<comment>租金支付方式</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.estate.preferences.enums.RentBillPlanEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
		<property name="totalAmount" type="java.lang.Double">
			<column name="total_amount" precision="10" scale="2">
				<comment>合同总额</comment>
			</column>
		</property>
		
		<property name="propertyName" type="java.lang.String">
			<column name="property_name">
				<comment>本物业名称</comment>
			</column>
		</property>
		<property name="address" type="java.lang.String">
			<column name="address">
				<comment>座落位置</comment>
			</column>
		</property>
		<property name="propertyNo" type="java.lang.String">
			<column name="propertyNo">
				<comment>乙方所租房屋产权证号</comment>
			</column>
		</property>
		<many-to-one name="roomType" class="com.wiiy.core.entity.DataDict" foreign-key="fk_estate_contract_room_datadict_type" insert="false" update="false" fetch="join">
			<column name="room_type_id">
				<comment>房间类型</comment>
			</column>
		</many-to-one>
		<property name="roomTypeId" type="java.lang.String">
			<column name="room_type_id">
				<comment>房间类型外键</comment>
			</column>
		</property>
		<many-to-one name="contractParty" class="com.wiiy.core.entity.DataDict" foreign-key="fk_estate_contract_party_datadict_type" insert="false" update="false" fetch="join">
			<column name="contract_party_id">
				<comment>合同甲方</comment>
			</column>
		</many-to-one>
		<property name="contractPartyId" type="java.lang.String">
			<column name="contract_party_id">
				<comment>合同甲方外键</comment>
			</column>
		</property>
		<property name="propertyUnit" type="java.math.BigDecimal">
			<column name="property_unit">
				<comment>单价</comment>
			</column>
		</property>
		<property name="overallFloorage" type="java.math.BigDecimal">
			<column name="overall_floorage">
				<comment>总建筑面积</comment>
			</column>
		</property>
		<property name="rentAmount" type="java.lang.String">
			<column name="rentAmount">
				<comment>租金额度</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
    </class>
</hibernate-mapping>
