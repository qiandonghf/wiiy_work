<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.estate.entity.Complaint">
    	<meta attribute="class-description">企业投诉</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name">
				<comment>投诉人</comment>
			</column>
		</property>
		<property name="tel" type="java.lang.String">
			<column name="tel">
				<comment>联系电话</comment>
			</column>
		</property>
		<property name="customerId" type="java.lang.Long">
			<column name="customer_id">
				<comment>企业外键</comment>
			</column>
		</property>
        <property name="customerName" type="java.lang.String">
			<column name="customer_name">
				<comment>企业名称</comment>
			</column>
		</property>
		<property name="complaintTime" type="java.util.Date">
			<column name="complaint_time">
				<comment>投诉时间</comment>
			</column>
		</property>
		<property name="complaintObject" type="java.lang.String" length="1024">
			<column name="complaint_object">
				<comment>投诉对象</comment>
			</column>
		</property>
		<property name="content" type="java.lang.String" length="1024">
			<column name="content" length="512">
				<comment>投诉内容</comment>
			</column>
		</property>
		<property name="status">
            <column name="status">
				<comment>状态</comment>
			</column>
            <type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.estate.preferences.enums.ComplaintAcceptStatusEnum</param>
				<param name="type">12</param>
			</type>
        </property>
        <property name="importance">
        	<column name="importance">
				<comment>重要程度</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.common.preferences.enums.ProjectPriorityEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="accepter" type="java.lang.String" length="1024">
			<column name="accepter">
				<comment>受理人</comment>
			</column>
		</property>
		<property name="result" type="java.lang.String" length="1024">
			<column name="result" length="512">
				<comment>处理结果</comment>
			</column>
		</property>
		
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
