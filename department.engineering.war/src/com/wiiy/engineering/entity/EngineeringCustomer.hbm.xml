<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.engineering.entity.EngineeringCustomer">
    	<meta attribute="class-description">客商</meta> 
        <meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="foreign">
                <param name="property">customerInfo</param>
            </generator>
		</id>
		<one-to-one name="customerInfo" class="com.wiiy.engineering.entity.EngineeringCustomerInfo" foreign-key="fk_engineering_customer_info_entity" constrained="true" fetch="join">
        	<meta attribute="class-description">客商详细信息</meta> 
        </one-to-one>
        <property name="name" type="java.lang.String">
        	<column name="name" unique-key="true">
        		<comment>名称</comment>
        	</column>
        </property>
        <property name="code" type="java.lang.String">
        	<column name="code" unique-key="true">
        		<comment>编码</comment>
        	</column>
        </property>
		<property name="parkTime" type="java.util.Date">
        	<column name="park_time">
        		<comment>入园时间</comment>
        	</column>
        </property>
        <property name="shortName" type="java.lang.String">
        	<column name="short_name">
        		<comment>简称</comment>
        	</column>
        </property>
        <property name="userId" type="java.lang.Long">
        	<column name="user_id">
        		<comment>账号外键ID</comment>
        	</column>
        </property>
        <many-to-one name="user" class="com.wiiy.core.entity.User" foreign-key="fk_customer_user" insert="false" update="false" fetch="join">
			<column name="user_id">
				<comment>账号实体</comment>
			</column>
		</many-to-one>
        <property name="hostId" type="java.lang.Long">
        	<column name="host_id">
        		<comment>跟踪人外键ID</comment>
        	</column>
        </property>
        <many-to-one name="host" class="com.wiiy.core.entity.User" foreign-key="fk_customer_host" insert="false" update="false" fetch="join">
			<column name="host_id">
				<comment>跟踪人实体</comment>
			</column>
		</many-to-one>
        <property name="refereeId" type="java.lang.Long">
        	<column name="referee_id">
        		<comment>引进人外键ID</comment>
        	</column>
        </property>
        <many-to-one name="referee" class="com.wiiy.core.entity.User" foreign-key="fk_customer_referee" insert="false" update="false" fetch="join">
			<column name="referee_id">
				<comment>引进人实体</comment>
			</column>
		</many-to-one>
		<property name="time" type="java.util.Date">
			<column name="time">
				<comment>状态变更时间</comment>
			</column>
		</property>
		<many-to-one name="technic" class="com.wiiy.core.entity.DataDict" foreign-key="fk_customer_datadict_technic" insert="false" update="false" fetch="join">
			<column name="technic_id">
				<comment>技术领域实体</comment>
			</column>
		</many-to-one>
		<property name="technicId" type="java.lang.String">
			<column name="technic_id">
				<comment>技术领域外键</comment>
			</column>
		</property>
        
        <many-to-one name="source" class="com.wiiy.core.entity.DataDict" foreign-key="fk_customer_datadict_source" insert="false" update="false" fetch="join">
			<column name="source_id">
				<comment>企业来源实体</comment>
			</column>
		</many-to-one>
		<property name="sourceId" type="java.lang.String">
			<column name="source_id">
				<comment>企业来源外键</comment>
			</column>
		</property>
		
        <many-to-one name="enterpriseType" class="com.wiiy.core.entity.DataDict" foreign-key="fk_customer_datadict_enterpriseType" insert="false" update="false" fetch="join">
			<column name="enterpriseType_id">
				<comment>创业者归属实体</comment>
			</column>
		</many-to-one>
		<property name="enterpriseTypeId" type="java.lang.String">
			<column name="enterpriseType_id">
				<comment>创业者归属外键</comment>
			</column>
		</property>
        <property name="incubated">
			<column name="incubated" sql-type="varchar(50)">
				<comment>是否孵化企业</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.BooleanEnum</param>
				<param name="type">12</param>
			</type>		
		</property>
		<property name="type">
        	<column name="type">
				<comment>类型</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.common.preferences.enums.CustomerTypeEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="customerType">
        	<column name="customer_type">
				<comment>客商类型</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.common.preferences.enums.CustomerSupplierTypeEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="ext1" type="java.lang.String">
            <column name="ext1">
                <comment>扩展1</comment>
            </column>
        </property>
		<property name="ext2" type="java.lang.String">
            <column name="ext2">
                <comment>扩展2</comment>
            </column>
        </property>
		<property name="ext3" type="java.lang.String">
            <column name="ext3">
                <comment>扩展3</comment>
            </column>
        </property>
		<property name="ext4" type="java.lang.String">
            <column name="ext4">
                <comment>扩展4</comment>
            </column>
        </property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
