<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.engineering.entity.EngineeringContractAtt">
    	<meta attribute="class-description">工程部合同附件</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.BaseEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="contractId" type="java.lang.Long">
			<column name="contract_id">
				<comment>合同外键</comment>
			</column>
		</property>
		<many-to-one name="contract" class="com.wiiy.engineering.entity.EngineeringContract" foreign-key="fk_contract_att_contract" insert="false" update="false" fetch="join">
            <column name="contract_id">
                <comment>合同</comment>
            </column>
        </many-to-one>
		<property name="type">
        	<column name="type">
				<comment>文件类别</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.common.preferences.enums.ContractAttTypeEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="name" type="java.lang.String">
			<column name="name">
				<comment>附件名称</comment>
			</column>
		</property>
		<property name="newName" type="java.lang.String">
			<column name="new_name">
				<comment>重命名名称</comment>
			</column>
		</property>
		<property name="size" type="java.lang.Long">
			<column name="size">
				<comment>附件大小</comment>
			</column>
		</property>
		<property name="memo" type="java.lang.String">
			<column name="memo">
				<comment>备注说明</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
