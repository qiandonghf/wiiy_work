<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.wiiy.synthesis.entity.CardCategory">
    	<meta attribute="class-description">名片分类</meta> 
    	<meta attribute="extends">com.wiiy.commons.entity.TreeEntity</meta>
		<id name="id" unsaved-value="-1" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="parentId" type="java.lang.Long" not-null="true">
			<column name="parent_id">
				<comment>父节点外键</comment>
			</column>
		</property>
		<property name="parentIds" type="java.lang.String" not-null="true">
			<column name="parent_ids">
				<comment>根目录开始的所有父节点</comment>
			</column>
		</property>
		<many-to-one name="parent" class="com.wiiy.synthesis.entity.CardCategory" foreign-key="fk_cardCategory_parent" insert="false" update="false" fetch="join">
			<column name="parent_id">
				<comment>文件夹实体</comment>
			</column>
		</many-to-one>
		
		<property name="name" type="java.lang.String" not-null="true" length="255">
			<column name="name" not-null="true">
				<comment>分类名称</comment>
			</column>
		</property>
		
		
		<property name="ownerId" type="java.lang.Long">
			<column name="owner_id">
				<comment>分类拥有者ID</comment>
			</column>
		</property>
		
		<property name="ownerEnum">
        	<column name="owner_enum">
				<comment>拥有类型</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.synthesis.preferences.enums.CardOwnerEnum</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="level" type="java.lang.Integer">
			<column name="level">
				<comment>LEVEL</comment>
			</column>
		</property>
		
		<property name="entityStatus">
        	<column name="entity_status">
				<comment>实体状态</comment>
			</column>
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">com.wiiy.commons.preferences.enums.EntityStatus</param>
				<param name="type">12</param>
			</type>
		</property>
		<property name="createTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="create_time">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="creator" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="creator">
				<comment>创建人姓名</comment>
			</column>
		</property>
		<property name="creatorId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="creator_id">
				<comment>创建人ID</comment>
			</column>
		</property>
		<property name="modifyTime" type="java.util.Date">
			<meta attribute="gen-property">false</meta>
			<column name="modify_time">
				<comment>修改时间</comment>
			</column>
		</property>
		<property name="modifier" type="java.lang.String" length="100">
			<meta attribute="gen-property">false</meta>
			<column name="modifier">
				<comment>修改人姓名</comment>
			</column>
		</property>
		<property name="modifierId" type="java.lang.Long">
			<meta attribute="gen-property">false</meta>
			<column name="modifier_id">
				<comment>修改人ID</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
